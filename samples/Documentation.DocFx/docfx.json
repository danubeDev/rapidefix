{
  "metadata": [
    {
      "src": [
        {
          "src": "../../src",
          "files": [ "**/*.csproj" ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "articles/toc.yml",
          "benchmarks/**github.md",
          "benchmarks/results/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../../docs",
    "template": [
      "default"
    ],
    "globalMetadata": {
      "_appTitle": "RapideFix",
      "_appFooter": "DanubeDev.RapideFix - Documentation",
      "_appLogoPath": "images/icon.svg",
      "_enableSearch": "true"
    }
  }
}